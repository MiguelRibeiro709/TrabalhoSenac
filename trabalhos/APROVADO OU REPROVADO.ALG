Algoritmo "Aprovado ou reprovado"

Var
   nota: caractere


Inicio
   escreval("escreva a sua nota")
   leia(nota)
   se (nota = "7") ou (nota = > 7) entao
   senao (nota = < 7 ) entao
      escreval ("Reprovado")
      se (nota = "10") entao
         Escreval ("Aprovado com distinção")
      fimse
   fimse


Fimalgoritmo